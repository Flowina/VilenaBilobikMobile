<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="EPAM mobile cloud Web testing Suite">
    <parameter name="appType" value="web"></parameter>
    <!--parameter name="udid" value="00008030-0000798A0EF2802E"></parameter-->
    <parameter name="udid" value="00008030-001115E40EE2802E"></parameter>
    <parameter name="platformName" value="iOS"></parameter>
    <parameter name="browserName" value="Safari"></parameter>
    <parameter name="baseUrl" value="https://www.google.com/"></parameter>
    <parameter name="searchText" value="Epam"></parameter>
    <test name="Cloud Web tests">
        <groups>
            <run>
                <include name="web"/>
            </run>
        </groups>
        <packages>
            <package name="scenarios"/>
        </packages>
    </test>
</suite>
